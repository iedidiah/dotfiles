[color]
	ui = true
[push]
	default=current
[alias]
	sla = log --oneline --decorate --graph --all
	plog = log --pretty=format:'%C(yellow)%h%C(reset) %an [%C(green)%ar%C(reset)] %s'
	glog = log -E -i --grep
	unstage = reset HEAD --
	uncommit = reset --soft HEAD^
	cm = commit -m 
	cob = checkout -b 
	co = checkout 
	unchange = checkout --
	mup = !git checkout master && git pull && git checkout -
